        "total_clicks": {
          "required": true,
          "aliases": ["Totalt antal klick", "Total clicks"],
          "type": "number",
          "aggregationType": "sum",
          "description": "Totalt antal klick"
        },
        "other_clicks": {
          "required": true,
          "aliases": ["Övriga klick", "Other clicks"],
          "type": "number",
          "aggregationType": "sum",
          "description": "Antal övriga klick"
        },
        "link_clicks": {
          "required": true,
          "aliases": ["Länkklick", "Link clicks"],
          "type": "number",
          "aggregationType": "sum",
          "description": "Antal klick på länkar"
        }
      }
    }
  }
}