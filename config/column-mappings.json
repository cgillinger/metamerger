{
  "version": "1.0",
  "categories": {
    "metadata": {
      "title": "Metadata",
      "description": "Grundläggande information om inlägget",
      "fields": {
        "postId": {
          "required": true,
          "aliases": ["Publicerings-id", "Post ID", "Inläggs-ID", "PostID"],
          "type": "string",
          "description": "Unikt ID för inlägget"
        },
        "accountId": {
          "required": true,
          "aliases": ["Sid-id", "Account ID", "Konto-ID", "KontoID"],
          "type": "string",
          "description": "ID för Facebook-sidan"
        },
        "accountName": {
          "required": true,
          "aliases": ["Sidnamn", "Account name", "Kontonamn"],
          "type": "string",
          "description": "Namnet på Facebook-sidan"
        }
      }
    },
    "metrics": {
      "title": "Mätvärden",
      "description": "Grundläggande mätvärden för inlägg",
      "fields": {
        "views": {
          "required": true,
          "aliases": ["Visningar", "Views"],
          "type": "number",
          "aggregationType": "sum",
          "description": "Antal visningar"
        }
      }
    },
    "reach": {
      "title": "Räckvidd",
      "description": "Mätvärden för inläggets räckvidd",
      "fields": {
        "reach": {
          "required": true,
          "aliases": ["Räckvidd", "Reach"],
          "type": "number",
          "aggregationType": "max",
          "description": "Räckvidd för inlägget"
        }
      }
    },
    "engagement": {
      "title": "Engagemang",
      "description": "Mätvärden för användarengagemang",
      "fields": {
        "total_engagement": {
          "required": true,
          "aliases": ["Reaktioner, kommentarer och delningar", "Total engagement"],
          "type": "number",
          "aggregationType": "sum",
          "description": "Totalt antal interaktioner"
        },
        "likes": {
          "required": true,
          "aliases": ["Reaktioner", "Likes", "Gilla-markeringar"],
          "type": "number",
          "aggregationType": "sum",
          "description": "Antal reaktioner"
        },
        "comments": {
          "required": true,
          "aliases": ["Kommentarer", "Comments"],
          "type": "number",
          "aggregationType": "sum",
          "description": "Antal kommentarer"
        },
        "shares": {
          "required": true,
          "aliases": ["Delningar", "Shares"],
          "type": "number",
          "aggregationType": "sum",
          "description": "Antal delningar"
